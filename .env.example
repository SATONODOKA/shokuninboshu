# LINE Bot Configuration
# LINE Channel Access Token (長期チャネルアクセストークン)
LINE_CHANNEL_ACCESS_TOKEN=your_channel_access_token_here

# LINE Channel Secret
LINE_CHANNEL_SECRET=your_channel_secret_here

# Frontend Environment Variables (VITE_ prefix required)
# LINE LIFF Application ID
VITE_LINE_LIFF_ID=your_liff_id_here

# LINE Channel ID (数値のみ)
VITE_LINE_CHANNEL_ID=your_channel_id_here

# API Base URL for Netlify Functions
VITE_API_BASE_URL=/.netlify/functions